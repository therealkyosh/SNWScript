task.wait(0.5)

local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local VirtualUser = game:GetService("VirtualUser")

local player = Players.LocalPlayer

-- Anti AFK
player.Idled:Connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)

-- UI
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.CoreGui
screenGui.ResetOnSpawn = false

local frame = Instance.new("Frame", screenGui)
frame.Size = UDim2.new(0, 320, 0, 150)
frame.Position = UDim2.new(0.5, -160, 0.5, -75)
frame.BackgroundTransparency = 0.2
frame.BackgroundColor3 = Color3.fromRGB(40,40,40)

local stroke = Instance.new("UIStroke", frame)
stroke.Thickness = 2
stroke.Color = Color3.new(1,1,1)

local text = Instance.new("TextLabel", frame)
text.Size = UDim2.new(1,-20,0,70)
text.Position = UDim2.new(0,10,0,10)
text.BackgroundTransparency = 1
text.Text = "This Script Is No Longer Available\nDo You Want To Execute Etherion Instead?"
text.TextWrapped = true
text.TextScaled = true
text.Font = Enum.Font.GothamBlack
text.TextColor3 = Color3.new(1,1,1)

local function createButton(txt, pos, callback)
    local btn = Instance.new("TextButton", frame)
    btn.Size = UDim2.new(0,130,0,40)
    btn.Position = pos
    btn.BackgroundTransparency = 0.2
    btn.BackgroundColor3 = Color3.fromRGB(60,60,60)
    btn.TextColor3 = Color3.new(1,1,1)
    btn.TextScaled = true
    btn.Font = Enum.Font.GothamBlack
    btn.Text = txt
    btn.MouseButton1Click:Connect(callback)
end

-- YES = Execute Etherion
createButton("Yes", UDim2.new(0,15,1,-50), function()
    screenGui:Destroy()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/therealkyosh/Etherion/refs/heads/main/%2B1SEFB"))()
end)

-- NO = Just destroy UI
createButton("No", UDim2.new(1,-145,1,-50), function()
    screenGui:Destroy()
end)
